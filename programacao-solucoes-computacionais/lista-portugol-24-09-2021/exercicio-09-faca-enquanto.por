programa {
	funcao inicio() {
		// 9. Faça um programa que imprima em ordem decrescente todos os números entre 40 e 20. 
		// Utilize estruturas de repetição e outras estruturas que julgar necessárias.
        
        inteiro contador = 40
        
        faca {
            escreva(contador + "\n")
            contador = contador - 1
        } enquanto (contador >= 20)
	}
}

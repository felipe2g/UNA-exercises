programa {
	funcao inicio() {
		// 10. Faça um programa que imprima todos os resultados da tabuada de 5. 
		// Utilize estruturas de repetição e outras estruturas que julgar necessárias.
        
        inteiro contador = 0
        
        faca {
            escreva("5 x " + contador + " = " + contador*5 + "\n")
            contador = contador + 1
        } enquanto (contador <= 10)
	}
}

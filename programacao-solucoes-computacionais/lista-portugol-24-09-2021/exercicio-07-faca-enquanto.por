programa {
	funcao inicio() {
		// Faça um programa que imprima em ordem crescente todos os números entre 100 e 200. 
		// Utilize estruturas de repetição.
        
        inteiro contador = 100
        
        faca {
            escreva(contador + "\n")
            contador = contador + 1
        } enquanto (contador <= 200)
	}
}

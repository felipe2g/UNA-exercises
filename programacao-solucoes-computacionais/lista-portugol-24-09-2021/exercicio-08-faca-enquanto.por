programa {
	funcao inicio() {
		// Faça um programa que imprima todos os números ímpares entre 500 e 530. 
		//Utilize estruturas de repetição e outras estruturas que julgar necessárias
        
        inteiro contador = 500
        
        faca {
            se(contador % 2 != 0) {
                escreva(contador + "\n")
            }
            contador = contador + 1
        } enquanto (contador <= 530)
	}
}
